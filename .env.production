# Production Environment Variables
# Replace these values with proper production secrets before deployment

# Database
POSTGRES_USER=postgres
POSTGRES_PASSWORD=replace_with_strong_password
POSTGRES_DB=astroshield

# API
PORT=3001
SECRET_KEY=replace_with_strong_secret_key
ACCESS_TOKEN_EXPIRE_MINUTES=30
ENVIRONMENT=production

# UDL Integration
UDL_BASE_URL=https://udl-service-prod.example.com/api/v1
UDL_USERNAME=replace_with_real_username
UDL_PASSWORD=replace_with_real_password

# CORS
CORS_ORIGINS=https://asttroshield.com,https://www.asttroshield.com

# Default Users (CHANGE FOR PRODUCTION)
ADMIN_PASSWORD=replace_with_strong_admin_password
TEST_PASSWORD=replace_with_strong_test_password

# Frontend
NEXT_PUBLIC_API_URL=https://asttroshield.com/api/v1
NODE_ENV=production 