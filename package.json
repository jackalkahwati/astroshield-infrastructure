{
  "name": "astroshield-microservice",
  "version": "1.0.0",
  "description": "Tests for AstroShield Microservice",
  "main": "index.js",
  "private": true,
  "scripts": {
    "test": "NODE_ENV=test jest --runInBand --setupFiles ./scripts/setup-test-env.js",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "build": "mkdir -p dist && cp -r src/* dist/ && echo 'Build completed'",
    "start": "node dist/main.js",
    "dev": "nodemon src/main.js",
    "migrate:up": "knex migrate:latest",
    "migrate:down": "knex migrate:rollback",
    "setup:test": "./scripts/setup-test-db.sh"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/static/**",
      "!**/templates/**"
    ],
    "globalSetup": "./scripts/jest-global-setup.js",
    "globalTeardown": "./scripts/jest-global-teardown.js"
  },
  "keywords": [],
  "author": "Stardrive Inc.",
  "license": "UNLICENSED",
  "devDependencies": {
    "axios": "^1.6.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/exporter-jaeger": "^1.18.1",
    "@opentelemetry/resources": "^1.18.1",
    "@opentelemetry/sdk-trace-base": "^1.18.1",
    "@opentelemetry/sdk-trace-node": "^1.18.1",
    "@opentelemetry/semantic-conventions": "^1.18.1",
    "@sentry/nextjs": "^8.47.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "csurf": "^1.11.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "pg": "^8.11.3",
    "prom-client": "^15.1.0",
    "winston": "^3.11.0"
  }
}
