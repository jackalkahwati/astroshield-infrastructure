# Vantiq Integration Environment Configuration

# Vantiq API Configuration
VANTIQ_API_URL=https://dev.vantiq.com/api/v1
VANTIQ_API_TOKEN=your_api_token_here
VANTIQ_NAMESPACE=asttroshield

# Vantiq Authentication
VANTIQ_USERNAME=your_username
VANTIQ_PASSWORD=your_password
VANTIQ_CLIENT_ID=your_client_id
VANTIQ_CLIENT_SECRET=your_client_secret

# Vantiq Topics
VANTIQ_TRAJECTORY_TOPIC=TRAJECTORY_UPDATES
VANTIQ_THREAT_TOPIC=THREAT_DETECTIONS
VANTIQ_COMMAND_TOPIC=COMMANDS

# Vantiq Webhook Configuration
VANTIQ_WEBHOOK_ENABLED=true
VANTIQ_WEBHOOK_SECRET=your_webhook_secret
VANTIQ_WEBHOOK_ENDPOINT=/api/vantiq/webhook

# Vantiq Retry Configuration
VANTIQ_MAX_RETRIES=3
VANTIQ_RETRY_DELAY=1000
VANTIQ_TIMEOUT=5000 